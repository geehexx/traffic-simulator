    .git/*
    tests/*
    scripts/*
    runs/*

[html]
directory = runs/coverage/htmlcov

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
